{
  "name": "node-service-starter",
  "version": "1.0.0",
  "description": "node backend service starter",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:el-ground/node-service-starter.git",
  "author": "<jwjang@elground.com>",
  "license": "MIT",
  "engines": {
    "node": "16"
  },
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist temp",
    "set-env": " echo set environment and temp files here!",
    "dev": "cross-env PORT=9023 HTTPS_PORT=9646 NODE_ENV=development CERT=$DEV_CERT CERT_KEY=$DEV_CERT_KEY nodemon dist/src/index.js",
    "package": "yarn clean && tsc --project tsconfig.src.json --skipLibCheck",
    "_package:listen": "yarn clean && yarn setenv && tsc --project tsconfig.src.json --watch --skipLibCheck",
    "package:listen": "cross-env ENVIRONMENT=development yarn _package:listen",
    "typecheck": "tsc --project tsconfig.src.json --skipLibCheck",
    "lint:js": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|jsx|mjs)\"",
    "prettier": "prettier --check --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|jsx|mjs)\"",
    "lint": "yarn lint:js & yarn prettier",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watchAll --coverage --passWithNoTests",
    "start": "cross-env NODE_ENV=production node dist/src/index.js",
    "_deploy": "yarn clean && yarn setenv && yarn package && gcloud app deploy gae-deployment.yaml",
    "deploy:staging": "cross-env ENVIRONMENT=staging yarn _deploy",
    "deploy:production": "cross-env ENVIRONMENT=production yarn _deploy"
  },
  "imports": {
    "#src/*": "./dist/src/*",
    "#framework/*": "./dist/src/framework/*",
    "#routers/*": "./dist/src/routers/*",
    "#model/*": "./dist/src/model/*",
    "#temp/*": "./dist/temp/*"
  },
  "devDependencies": {
    "@types/cls-hooked": "^4.3.3",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-request-id": "^1.4.3",
    "@types/ip": "^1.1.0",
    "@types/jest": "^28.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "chokidar-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^28.1.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-request-id": "^2.0.1",
    "ip": "^1.1.8",
    "morgan": "^1.10.0",
    "node-fetch": "^3.2.6",
    "source-map-support": "^0.5.21",
    "winston": "^3.7.2"
  }
}
