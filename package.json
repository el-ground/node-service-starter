{
  "name": "node-service-starter",
  "version": "1.0.0",
  "description": "node backend service starter",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:el-ground/node-service-starter.git",
  "author": "<jwjang@elground.com>",
  "license": "MIT",
  "engines": {
    "node": "16"
  },
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "set-env": " echo set environment and temp files here!",
    "dev": "cross-env PORT=9023 HTTPS_PORT=9646 NODE_ENV=development CERT=$DEV_CERT CERT_KEY=$DEV_CERT_KEY nodemon dist/src/index.js",
    "package": "tsc --project tsconfig.src.json",
    "_package:listen": "yarn clean && yarn set-env && tsc --project tsconfig.src.json --watch",
    "package:listen": "cross-env ENVIRONMENT=development yarn _package:listen",
    "typecheck": "tsc --project tsconfig.src.json",
    "lint:js": "eslint --ignore-path .eslintignore \"src/**/*.+(ts|js|tsx|jsx|mjs)\"",
    "prettier": "prettier --check --ignore-path .eslintignore \"src/**/*.+(ts|js|tsx|jsx|mjs)\"",
    "lint": "yarn lint:js & yarn prettier",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watchAll --coverage --passWithNoTests",
    "start": "cross-env NODE_ENV=production node dist/src/index.js",
    "_deploy": "yarn clean && yarn set-env && yarn package && gcloud app deploy gae-deployment.yaml",
    "deploy:staging": "cross-env ENVIRONMENT=staging yarn _deploy",
    "deploy:production": "cross-env ENVIRONMENT=production yarn _deploy"
  },
  "imports": {
    "#src/*": "./dist/src/*"
  },
  "devDependencies": {
    "@types/cls-hooked": "^4.3.3",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-request-id": "^1.4.3",
    "@types/ip": "^1.1.0",
    "@types/jest": "^28.1.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.2",
    "@types/node-fetch": "^2.6.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "chokidar-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-request-id": "^2.0.1",
    "ip": "^1.1.8",
    "morgan": "^1.10.0",
    "node-fetch": "^3.2.10",
    "winston": "^3.8.1"
  }
}
